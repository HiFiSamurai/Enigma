{
    "parser": "babel-eslint",
    "root": true,
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": ["error", "always"],
        "space-before-function-paren": 0,
        "indent": ["error", 2],
        "comma-dangle": ["error", "always-multiline"],
        "brace-style": 0
    }
}
